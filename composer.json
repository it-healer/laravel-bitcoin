{
    "name": "it-healer/laravel-bitcoin",
    "description": "A library for Laravel that allows you to create and manage the Bitcoin cryptocurrency.",
    "homepage": "https://github.com/it-healer/laravel-bitcoin",
    "license": "MIT",
    "authors": [
        {
            "name": "IT-HEALER",
            "email": "info@it-healer.com"
        }
    ],
    "keywords": [
        "it-healer",
        "laravel",
        "bitcoin",
        "btc",
        "php"
    ],
    "require": {
        "php": "^8.2",
        "ext-decimal": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.92.0"
    },
    "autoload": {
        "psr-4": {
            "ItHealer\\LaravelBitcoin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ItHealer\\LaravelBitcoin\\BitcoinServiceProvider"
            ],
            "aliases": {
                "Bitcoin": "ItHealer\\LaravelBitcoin\\Facades\\Bitcoin"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
